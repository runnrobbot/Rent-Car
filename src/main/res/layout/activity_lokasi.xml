<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".lokasiActivity">

    <LinearLayout
        android:id="@+id/linearLayout21"
        android:layout_width="408dp"
        android:layout_height="94dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="40dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/listView1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="146dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#E91E63"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageBtnBack"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:backgroundTint="#FD0505"
                android:tint="@color/white"
                app:srcCompat="?attr/homeAsUpIndicator" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="413dp"
            android:layout_height="96dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView25"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#FD0505"
                android:fontFamily="sans-serif-black"
                android:text="ENF MOBILINDO"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="36sp"
                app:autoSizeTextType="none" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout19"
        android:layout_width="409dp"
        android:layout_height="538dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <fragment
            android:id="@+id/mapView2"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="425dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="34dp" />

        <Button
            android:id="@+id/btnSelesaiLokasi"
            android:layout_width="match_parent"
            android:layout_height="67dp"
            android:backgroundTint="#FD0505"
            android:text="Kembali"
            android:textColor="@color/white" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>